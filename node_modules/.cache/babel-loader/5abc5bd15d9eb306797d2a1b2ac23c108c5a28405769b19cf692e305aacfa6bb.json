 = useState('');\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    if (username === 'admin' && password === 'admin123') {\n      onLogin();\n    } else {\n      setError('Invalid credentials');\n    }\n  };\n\n  return (\n    <div className=\"registration-modal\">\n      <div className=\"registration-content\">\n        <h2>Admin Login</h2>\n        <form onSubmit={handleSubmit} className=\"registration-form\">\n          <label>Username:\n            <input type=\"text\" value={username} onChange={e => setUsername(e.target.value)} required />\n          </label>\n          <label>Password:\n            <input type=\"password\" value={password} onChange={e => setPassword(e.target.value)} required />\n          </label>\n          {error && <div className=\"error-message\" style={{color:'red',marginBottom:'1rem'}}>{error}</div>}\n          <button type=\"submit\" className=\"btn-primary\">Login</button>\n        </form>\n      </div>\n    </div>\n  );\n};\n\nexport default AdminLogin;\n"],"mappings":"AAAA,MAAO,CAAAA,KAAK,EAAIC,QAAQ,KAAQ,OAAO,CACvC,MAAO,YAAY,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAEpB,KAAM,CAAAC,UAAU,CAAGC,IAAA,EAAiB,IAAhB,CAAEC,OAAQ,CAAC,CAAAD,IAAA,CAC7B,KAAM,CAACE,QAAQ,CAAEC,WAAW,CAAC,CAAGT,QAAQ,CAAC,EAAE,CAAC,CAC5C,KAAM,CAACU,QAAQ,CAAEC,WAAW,CAAC,CAAGX,QAAQ,CAAC,EAAE,CAAC,CAC5C,KAAM,CAACY,KAAK,CAAEC,QAAQ,CAAC,CAAGb,QAAQ,CAAC,EAAE,CAAC,CAEtC,KAAM,CAAAc,YAAY,CAAIC,CAAC,EAAK,CAC1BA,CAAC,CAACC,cAAc,CAAC,CAAC,CAClB,GAAIR,QAAQ,GAAK,OAAO,EAAIE,QAAQ,GAAK,UAAU,CAAE,CACnDH,OAAO,CAAC,CAAC,CACX,CAAC,IAAM,CACLM,QAAQ,CAAC,qBAAqB,CAAC,CACjC,CACF,CAAC,CAED,mBACEX,IAAA,QAAKe,SAAS,CAAC,oBAAoB,CAAAC,QAAA,cACjCd,KAAA,QAAKa,SAAS,CAAC,sBAAsB,CAAAC,QAAA,eACnChB,IAAA,OAAAgB,QAAA,CAAI,aAAW,CAAI,CAAC,cACpBd,KAAA,SAAMe,QAAQ,CAAEL,YAAa,CAACG,SAAS,CAAC,mBAAmB,CAAAC,QAAA,eACzDd,KAAA,UAAAc,QAAA,EAAO,WACL,cAAAhB,IAAA,UAAOkB,IAAI,CAAC,MAAM,CAACC,KAAK,CAAEb,QAAS,CAACc,QAAQ,CAAEP,CAAC,EAAIN,WAAW,CAACM,CAAC,CAACQ,MAAM,CAACF,KAAK,CAAE,CAACG,QAAQ,MAAE,CAAC,EA